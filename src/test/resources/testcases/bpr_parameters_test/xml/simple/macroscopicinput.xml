<PLANit xmlns:gml="http://www.opengis.net/gml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../main/resources/xsd/macroscopicinput.xsd">
	<macroscopicdemand>
		<demandconfiguration>
			<userclasses>
				<userclass id="1" moderef="1">
					<name>1</name>
				</userclass>
			</userclasses>
			<timeperiods>
				<timeperiod id="0">
					<name>Time_Period_1</name>
					<starttime>00:00:01</starttime>
					<duration>86400</duration>
				</timeperiod>
			</timeperiods>
		</demandconfiguration>
		<oddemands>
			<odcellbycellmatrix timeperiodref="0"	userclassref="1">
				<o ref="1">
					<d ref="2">2000</d>
				</o>
			</odcellbycellmatrix>
		</oddemands>
	</macroscopicdemand>
	<macroscopicnetwork>
		<linkconfiguration>                                                                        
			<modes>                                                                                     
				<mode id="1">																
					<name>Basic</name>												
					<pcu>1</pcu>															
				</mode>
			</modes>
			<linksegmenttypes>                            
				<linksegmenttype id="1">                    
					<name>Slow</name>													
					<capacitylane>1000</capacitylane>					
					<access>                                   
						<mode ref="1">													
							<maxspeed>10</maxspeed>								
						</mode>
					</access>
				</linksegmenttype>
				<linksegmenttype id="2">                    
					<name>Fast</name>													
					<capacitylane>1000</capacitylane>							
					<access>                                       
						<mode ref="1">															
							<maxspeed>20</maxspeed>										
						</mode>
					</access>
				</linksegmenttype>
			</linksegmenttypes>
		</linkconfiguration>
		<infrastructure>
			<nodes>
				<node id="1" />
				<node id="2" />
				<node id="3" />
				<node id="4" />
				<node id="5" />
				<node id="6" />
			</nodes>
			<links>
				<link nodearef="1" nodebref="2">
					<linksegment id="1" dir="a_b" typeref="1">
						<numberoflanes>1</numberoflanes>
					</linksegment>
					<length>10</length>
				</link>
				<link nodearef="2" nodebref="3">
					<linksegment id="2" dir="a_b" typeref="2">
						<numberoflanes>1</numberoflanes>
					</linksegment>
					<length>10</length>
				</link>
				<link nodearef="3" nodebref="4">
					<linksegment id="3" dir="a_b" typeref="1">
						<numberoflanes>1</numberoflanes>
					</linksegment>
					<length>10</length>
				</link>
				<link nodearef="4" nodebref="5">
					<linksegment id="4" dir="a_b" typeref="2">
						<numberoflanes>1</numberoflanes>
					</linksegment>
					<length>10</length>
				</link>
				<link nodearef="5" nodebref="6">
					<linksegment id="5" dir="a_b" typeref="1">
						<numberoflanes>1</numberoflanes>
					</linksegment>
					<length>10</length>
				</link>
			</links>
		</infrastructure>
	</macroscopicnetwork>
	<macroscopiczoning>
		<zones>
			<zone id="1">
				<centroid>
					<name>Start Zone</name>
				</centroid>
				<connectoids>
					<connectoid noderef="1">
					</connectoid>
				</connectoids>
			</zone>
			<zone id="2">
				<centroid>
					<name>Finish Zone</name>
				</centroid>
				<connectoids>
					<connectoid noderef="6">
					</connectoid>
				</connectoids>
			</zone>
		</zones>
	</macroscopiczoning>
</PLANit>